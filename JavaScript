// Function
print('Hello Class');

// Variables
var city = 'Katsina';
print(city);
var population = 524000;
print(population);

// List
var majorLGA = ['Daura', 'Funtua', 'Dutsinma', 'Malumfashi', 'Kankia', 'Mani', 'Katsina'];
print(majorLGA)

// Dictionary
var cityData = {'city' : city, 
  'population' : 524000, 
  'elevation' : 513};
print(cityData)

// Functions
var greet = function(name) {
  var greeting = 'Hello ' + name 
  return greeting
}

print(greet('Class'))